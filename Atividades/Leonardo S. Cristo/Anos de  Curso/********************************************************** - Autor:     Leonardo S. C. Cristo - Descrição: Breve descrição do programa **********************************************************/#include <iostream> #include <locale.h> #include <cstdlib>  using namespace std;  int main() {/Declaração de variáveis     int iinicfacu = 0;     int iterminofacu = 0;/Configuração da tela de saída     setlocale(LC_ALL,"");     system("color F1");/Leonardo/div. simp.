using namespace std;

int main()
{
    //Declaração de variáveis
    float fnum1 = 0;
    float fnum2 = 0;
    float fnum3 = 0;
   

    //Configuração da tela de saída
    setlocale(LC_ALL,"");
    system("color F1");

    //Código do programa
      cout << "Entre com 1° numero ";
      cin >> fnum1 ;

      cout << "Entre com 2° numero ";
      cin >> fnum2 ;

  

       cout << "Resultado " << fnum1 / fnum2   << ".";




